{
    "name": "samtarling/wpgraphs",
    "description": "Does API queries against https://en.wikipedia.org, updates a database with counts of things and shows pretty graphs",
    "license": "MIT",
    "require": {
        "squizlabs/php_codesniffer": "^3.5",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "WikiApi\\": "WikiApi",
            "WikiLogIt\\": "WikiLogIt"
        }
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.2"
    },
    "scripts": {
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
    },
    "support": {
        "issues": "https://github.com/samtarling/wpGraphs/issues",
        "source": "https://github.com/samtarling/wpGraphs"
    }
}
