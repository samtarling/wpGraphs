{
    "require": {
        "squizlabs/php_codesniffer": "^3.5",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "WikiApi\\": "WikiApi",
            "WikiLogIt\\": "WikiLogIt"
